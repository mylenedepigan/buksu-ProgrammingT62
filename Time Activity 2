#include<stdio.h>
int main()
{ 
int ID,PIN;
 printf("Enter ID number:");
 scanf("%d",&ID);
 printf("Enter PIN:");
 scanf("%d",&PIN); 
if(ID>=7600){
 printf("You have successfully logged in \nID#:1500\n"); 
 } 
else { 
   printf("\tInvalid ID/PIN!\n"); 
} 
int IdNoandPin[3] [2]= {{1,2},{7,4},{3,9},};
    
int i,j;
 for(i=0; i<3; i++) {
    for(j=0; j<2; j++) {
    printf(" ID#&PIN [%d] [%d]: %d\n",IdNoandPin[3] [2]);
  }
   } 
 
return 0;
 }
